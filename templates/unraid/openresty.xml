<?xml version="1.0"?>
<Container version="2">
  <Name>OpenResty</Name>
  <Repository>registry.cn-chengdu.aliyuncs.com/clion/openresty</Repository>
  <Registry/>
  <Network>vnet</Network>
  <MyIP/>
  <Shell>sh</Shell>
  <Privileged>false</Privileged>
  <Support>https://github.com/clion007/docker-openresty/issues/new/choose</Support>
  <Project>https://github.com/clion007/docker-openresty</Project>
  <Overview>openresty [engine x] is an HTTP and reverse proxy server, a mail proxy server, and a generic TCP/UDP proxy server, originally written by Igor Sysoev.</Overview>
  <Category>Network:Web Network:Proxy</Category>
  <WebUI/>
  <TemplateURL>https://raw.githubusercontent.com/clion007/docker-openresty/main/templates/unraid/openresty.xml</TemplateURL>
  <Icon>https://openresty.org/images/logo.png</Icon>
  <ExtraParams/>
  <PostArgs/>
  <CPUset/>
  <DonateText/>
  <DonateLink/>
  <Requires/>
  <Config Name="&#x914D;&#x7F6E;&#x8DEF;&#x5F84;" Target="/config" Default="" Mode="rw" Description="Contains your www content and all relevant configuration files." Type="Path" Display="advanced" Required="true" Mask="false">/mnt/user/appdata</Config>
  <Config Name="PUID" Target="PUID" Default="1000" Mode="{3}" Description="" Type="Variable" Display="advanced" Required="true" Mask="false">99</Config>
  <Config Name="PGID" Target="PGID" Default="1000" Mode="{3}" Description="" Type="Variable" Display="advanced" Required="true" Mask="false">100</Config>
  <Config Name="UMASK" Target="UMASK" Default="022" Mode="{3}" Description="" Type="Variable" Display="advanced" Required="false" Mask="false">022</Config>
</Container>